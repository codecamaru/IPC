<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<BorderPane fx:id="borderPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="poiupv.PrincipalController">
   <top>
      <HBox prefHeight="36.0" prefWidth="590.0" spacing="10.0" BorderPane.alignment="TOP_RIGHT">
         <children>
            <Button fx:id="estadisticas" mnemonicParsing="false" onAction="#muestraVentana" text="Estadísticas" />
            <Button fx:id="escogerProblema" mnemonicParsing="false" onAction="#muestraVentana" text="Escoger un problema" />
            <Button fx:id="mapa" mnemonicParsing="false" onAction="#muestraVentana" text="Mapa" />
            <Separator orientation="VERTICAL" prefHeight="200.0" />
            <Button fx:id="enunciado" mnemonicParsing="false" onAction="#muestraVentana" text="Enunciado" />
            <Region HBox.hgrow="ALWAYS" />
            <MenuButton mnemonicParsing="false" text="Usuario">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#inicioRegistro" text="Inicio de sesión" />
                <MenuItem fx:id="verEditarPerfil" mnemonicParsing="false" onAction="#verEditarPerfil" text="Ver/Editar perfil" />
                  <MenuItem fx:id="cerrarSesion" mnemonicParsing="false" onAction="#cerrarSesion" text="Cerrar sesión" />
              </items>
            </MenuButton>
         </children>
      </HBox>
   </top>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</BorderPane>
